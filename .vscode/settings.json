{
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.env": true,
        "**/*.egg-info": true,
        "**/.pytest_cache": true,
        "**/.coverage": true,
        "**/htmlcov": true,
        "**/.mypy_cache": true,
        "**/.tox": true,
        "**/dist": true,
        "**/build": true,
        "**/*.log": true
    },
    "search.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.env": true,
        "dataset/raw/": true,
        "**/*.egg-info": true,
        "**/.pytest_cache": true,
        "**/.coverage": true,
        "**/htmlcov": true,
        "**/.mypy_cache": true,
        "**/.tox": true,
        "**/dist": true,
        "**/build": true,
        "**/*.log": true
    },
    "python.terminal.activateEnvironment": true,
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "editor.formatOnSave": true,
}
